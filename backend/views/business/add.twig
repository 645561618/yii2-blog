{% set sidebar_name = "添加商家" %}
{% set menu_name = "business" %}
{% include "@app/views/layouts/header.twig" %}
<div class="content">
    <h3>添加商家</h3>
{% block content %}
<div class="container">

    <div class="row">
        <div class="form">
            
            {% set form = form_begin({'id':'create-form','options':{'enctype':'multipart/form-data','class':'form-horizontal'}}) %}
            {{ form.field(model,'username')|raw }}
            {{ form.field(model,'password').passwordInput()|raw }}
            {{ form.field(model,'phone')|raw }}
            <div class="form-group">
                        <input type='submit' value='提交' class='btn btn-large btn-primary' /> 
            </div>
            {{ void(form.end()) }}
        </div>
    </div>
</div>
</div>
{% endblock %}
</div>


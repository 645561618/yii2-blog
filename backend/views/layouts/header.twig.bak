	{% if sidebar_name is not empty %}
	<div id="sidebar-nav" style="position:fixed;top:50px;height:120%;background:#222;width:190px;float: left;text-align:left;text-indent:0px;">
        	<ul class="nav nav-list" id="dashboard-menu"><!--for value in this.context.sidebars-->
        		{% for key,value in App.getSubMenu(menu_name) %}
		            {% if value.subs_son is defined %}
                		<li>
		                    <a href="javascript:;" onclick="showList('{{ key }}');"><i class="icon-tasks"></i> <span class="glyphicon glyphicon-plus" id="list_k{{ key }}">&nbsp;<b style="font-weight: bolder;">{{ value.name }}</b></span></a>
                		    {% for k,v in value.subs_son %}
		                    	   {% set sidebar_route = v.route|split('/') %}
                			    <li class="theme-mobile-hack-{{ key }} {% if sidebar_name == v.name %}cur {% endif %}" style="display:none;">
		                           	<a href="/{{ v.route }}">{% if sidebar_name == v.name %}<i class="glyphicon glyphicon-chevron-right" style="left:95%"></i>{% else %}<i class="glyphicon glyphicon-chevron-left" style="right:95%"></i>{% endif  %}<span>{{ v.name }}</span></a>
                   	         	    </li>
                    		    {% endfor %}
                		</li>
            		    {% else %}
                		<li class=" theme-mobile-hack {% if sidebar_name == value.name %}cur {% endif %}" >
		                    <a href="/{{ value.route }}">{% if sidebar_name == value.name %}<i class="glyphicon glyphicon-chevron-right" style="left:95%"></i>{% else %}<i class="glyphicon glyphicon-chevron-left" style="right:95%"></i>{% endif  %} <span>{{ value.name }}</span></a>
                		</li>
            		   {% endif %}
        		{% endfor %}
       	        </ul>
	</div>

<script type="text/javascript">
function showList(num){
    $(".theme-mobile-hack-"+num).toggle();
    var valc = $("#list_k"+num).attr("class");
    if(valc == "glyphicon glyphicon-plus"){
        $("#list_k"+num).attr("class","glyphicon glyphicon-minus");
    }else{
        $("#list_k"+num).attr("class","glyphicon glyphicon-plus");
    }
}
$(function(){
    var val = $(".cur").attr("class");
    var array = val.split(" ");
    $("."+array[0]).show();
    var num = array[0].split("-");
    $("#list_k"+num[3]).attr("class","glyphicon glyphicon-minus");
});
</script>
